{
  "name": "nodejs-learn",
  "description": "A test project",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Adelin Niculae",
    "email": "adelin.nic3@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^16.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run compile && npm run mocha",
    "lint": "npx eslint src/. test/. --config .eslintrc.json --ext .ts --fix",
    "dev": "npx --loglevel verbose ts-node-dev --debug --no-notify src/",
    "serve": "node lib/",
    "start": "npm run compile && npm run serve",
    "mocha": "npx mocha --require ts-node/register --require source-map-support/register \"test/**/*.ts\" --recursive --exit",
    "compile": "npx shx rm -rf lib/ && npx tsc"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "types": "lib/",
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/feathersjs__configuration": "^1.0.5",
    "@types/feathersjs__express": "^1.1.9",
    "@types/node": "^18.0.1",
    "@types/serve-favicon": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@feathersjs/authentication-oauth": "^4.5.15",
    "@feathersjs/configuration": "^4.5.15",
    "@feathersjs/express": "^4.5.15",
    "@feathersjs/feathers": "^4.5.15",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-knex": "^8.0.1",
    "helmet": "^5.1.0",
    "knex": "^2.1.0",
    "pg": "^8.7.3",
    "serve-favicon": "^2.5.0",
    "winston": "^3.8.1"
  }
}
